{
  "name": "freezy-bee/datagrid-bundle",
  "type": "symfony-bundle",
  "description": "Symfony datagrid",
  "license": "MIT",
  "authors": [
    {
      "name": "Jakub Janata",
      "email": "jakubjanata@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "FreezyBee\\DataGridBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FreezyBee\\DataGridBundle\\Tests\\": "tests"
    }
  },
  "require": {
    "php": "^7.1",
    "ext-json": "*",
    "doctrine/orm": "^2.6",
    "symfony/config": "^4.2 || ^5.0",
    "symfony/dependency-injection": "^4.2 || ^5.0",
    "symfony/property-access": "^4.2 || ^5.0",
    "symfony/serializer": "^4.2 || ^5.0",
    "symfony/translation": "^4.2 || ^5.0",
    "symfony/yaml": "^4.2 || ^5.0",
    "twig/twig": "^2.6 || ^3.0"
  },
  "config": {
    "sort-packages": true
  },
  "require-dev": {
    "doctrine/doctrine-bundle": "^2.0",
    "phpstan/phpstan-shim": "^0.11",
    "phpunit/phpunit": "^7.5 || ~8.4",
    "squizlabs/php_codesniffer": "^3.4",
    "symfony/framework-bundle": "^5.0",
    "symfony/panther": "^0.6",
    "symfony/phpunit-bridge": "^5.0",
    "symfony/twig-bundle": "^5.0"
  }
}
